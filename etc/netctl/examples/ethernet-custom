Description='A more versatile static ethernet connection'
Interface=eth0
Connection=ethernet
IP=static
# Any valid iproute command can be placed in this array
IPCustom=('addr add dev eth0 192.168.1.23/24 brd +' 'route add default via 192.168.1.1')
DNS=('192.168.1.1')
